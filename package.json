{
  "name": "node-albion-api",
  "version": "2.1.2",
  "description": "Node Js client to interact with albion killboard API.",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "patch": "npm run build && npm version patch && npm publish",
    "format": "prettier --write '**/*.ts'",
    "build": "tsc",
    "generate-doc": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bassochette/albion-api"
  },
  "bugs": {
    "url": "https://github.com/bassochette/albion-api/issues"
  },
  "contributors": [
    {
      "name": "Julien Prugne",
      "email": "julien@webeleon.dev"
    }
  ],
  "keywords": [
    "albion online",
    "albion killboard",
    "albion API"
  ],
  "author": "yogourt <julien.prugne@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/sinon": "9.0.11",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "sinon": "9.2.4",
    "ts-jest": "26.5.3",
    "ts-node": "9.1.1",
    "typedoc": "0.20.31",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "query-string": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": "prettier --write"
  }
}
