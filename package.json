{
  "name": "node-albion-api",
  "version": "2.1.3",
  "description": "Node Js client to interact with albion killboard API.",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "patch": "npm run build && npm version patch && npm publish",
    "format": "prettier --write '**/*.ts'",
    "build": "tsc",
    "generate-doc": "typedoc",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bassochette/albion-api"
  },
  "bugs": {
    "url": "https://github.com/bassochette/albion-api/issues"
  },
  "contributors": [
    {
      "name": "Julien Prugne",
      "email": "julien@webeleon.dev"
    }
  ],
  "keywords": [
    "albion online",
    "albion killboard",
    "albion API"
  ],
  "author": "yogourt <julien.prugne@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/sinon": "10.0.2",
    "husky": "7.0.1",
    "jest": "27.0.4",
    "lint-staged": "11.0.0",
    "prettier": "2.3.0",
    "sinon": "11.1.1",
    "standard-version": "9.3.0",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0",
    "typedoc": "0.20.36",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "query-string": "^7.0.0"
  },
  "lint-staged": {
    "*.{ts}": "prettier --write"
  }
}
